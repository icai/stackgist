    "@types/mocha": "^2.2.40",
    "@types/next": "^6.1.7",
    "@types/node": "^7.0.12",
    "@types/react": "^16.4.14",
    "@types/supertest": "^2.0.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-assets": "^1.0.2",
    "cross-env": "^5.2.0",
