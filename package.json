{
  "private": true,
  "name": "stackgist",
  "version": "1.0.0",
  "description": "stackgist",
  "main": "index.js",
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-stackgist",
    "stop": "egg-scripts stop --title=egg-server-stackgist",
    "dev": "egg-bin dev -r egg-ts-helper/register",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov -r egg-ts-helper/register",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean",
    "ndev": "nodemon server/index.ts",
    "nbuild": "next build ./src && tsc --project tsconfig.server.json",
    "nstart": "cross-env NODE_ENV=production node ./.next/production-server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icai/stackgist.git"
  },
  "keywords": [
    "next.js",
    "stackgist",
    "egg"
  ],
  "egg": {
    "typescript": true
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "Terry Cai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icai/stackgist/issues"
  },
  "homepage": "https://github.com/icai/stackgist#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "css-loader": "^1.0.0",
    "dotenv-webpack": "^1.5.7",
    "egg": "^2.6.1",
    "egg-passport": "^2.0.1",
    "egg-passport-github": "^1.0.0",
    "egg-passport-local": "^1.2.1",
    "egg-passport-weibo": "^1.0.0",
    "egg-redis": "^2.0.0",
    "egg-router-plus": "^1.3.0",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^4.1.0",
    "file-loader": "^2.0.0",
    "immutability-helper": "^2.8.1",
    "koa": "^2.5.3",
    "koa-router": "^7.4.0",
    "less": "^3.8.1",
    "lodash": "^4.17.11",
    "lru-cache": "^4.1.3",
    "markdown": "^0.5.0",
    "mysql2": "^1.6.1",
    "next": "^7.0.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.12",
